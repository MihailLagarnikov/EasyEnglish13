<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools" package="ru.lagarnikov.easyenglish13">

    <uses-permission android:name="android.permission.INTERNET" />




    <application
            android:name=".MyApplication"
            android:allowBackup="true"
            android:icon="@drawable/ic_group_22"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".presentation.view.MainActivity"
                  android:theme="@style/AppTheme.Launcher"
                  android:configChanges="orientation"
                  android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <meta-data
                android:name="com.google.android.gms.ads.APPLICATION_ID"
                android:value="ca-app-pub-2421174998731562~1772735539"/>
        <meta-data android:name="preloaded_fonts" android:resource="@array/preloaded_fonts"/>




    </application>

</manifest>